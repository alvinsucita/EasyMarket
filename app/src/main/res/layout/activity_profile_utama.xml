<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BAFF4007"
    tools:context=".ProfileUtamaActivity">

    <TextView
        android:id="@+id/tvJudul"
        android:layout_width="221dp"
        android:layout_height="47dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="16dp"
        android:text="ProfileUser"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@android:color/white"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgViewProfileUtamaActivity"
        android:layout_width="196dp"
        android:layout_height="152dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="76dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/tvNamProfilUtamaActivity"
        android:layout_width="289dp"
        android:layout_height="42dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="252dp"
        android:text="Nama"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvEmailProfileUserActivity"
        android:layout_width="279dp"
        android:layout_height="44dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="304dp"
        android:text="Email"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvDaerahAsalProfileUserActivity"
        android:layout_width="285dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="360dp"
        android:text="DaerahAsal"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvUmurProfileUtamaActivity"
        android:layout_width="297dp"
        android:layout_height="45dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="420dp"
        android:text="Umur"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvGenderProfileUtamaActivity"
        android:layout_width="281dp"
        android:layout_height="46dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="480dp"
        android:text="Gender"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>